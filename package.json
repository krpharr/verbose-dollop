{
  "name": "google-book-search",
  "version": "1.0.0",
  "description": "Google book search MERN stack application",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "engines": {
    "node": "10.16.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krpharr/verbose-dollop.git"
  },
  "author": "rpharr@richmond.edu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/krpharr/verbose-dollop/issues"
  },
  "homepage": "https://github.com/krpharr/verbose-dollop#readme",
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.9.7",
    "path": "^0.12.7",
    "request": "^2.88.2",
    "socket.io": "^2.3.0"
  }
}
